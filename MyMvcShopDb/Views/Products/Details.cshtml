@using MyMvcShopDb.Core.Models
@model Product
@{
    ViewData["Title"] = Model.Name;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/landing-product.css" />
</head>
<body data-bs-spy="scroll" data-bs-target="#navSections" style="padding-top: 56px;">

    @await Html.PartialAsync("_LandingNavbar", Model)

    @await Html.PartialAsync("_LandingHero", Model)
    @await Html.PartialAsync("_LandingFeatures", Model)
    @await Html.PartialAsync("_LandingGallery", Model)


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>